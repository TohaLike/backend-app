generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id       String  @id @unique
  password String
  Token    Token[]
}

model Token {
  user         User   @relation(fields: [userId], references: [id])
  userId       String @unique
  refreshToken String @unique
}
